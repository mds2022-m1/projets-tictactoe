// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

//  user model
model Users {
  id          String   @id @default(uuid())
  full_name   String
  pseudo      String
  created_at  DateTime @default(now())
}
