{
	"name": "tictactoe",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --write . && eslint './src/**/*.{js,ts,svelte}'",
		"lint:fix": "prettier --write . && eslint --fix './src/**/*.{js,ts,svelte}'",
		"format": "prettier --plugin-search-dir . --write ."
	},
	"prisma": {
		"seed": "node --loader ts-node/esm prisma/seed.ts"
	},
	"devDependencies": {
		"@sveltejs/adapter-auto": "next",
		"@sveltejs/kit": "next",
		"@types/bcrypt": "^5.0.0",
		"@types/node": "^18.13.0",
		"autoprefixer": "^10.4.13",
		"eslint-config-typescript-airbnb-prettier-svelte": "^1.1.3",
		"eslint-plugin-eslint-comments": "^3.2.0",
		"eslint-plugin-prettier": "^4.2.1",
		"eslint-plugin-promise": "^6.1.1",
		"postcss": "^8.4.19",
		"svelte": "^3.53.1",
		"svelte-check": "^2.9.2",
		"svelte-preprocess": "^4.10.7",
		"tailwindcss": "^3.2.4",
		"ts-node": "^10.9.1",
		"tslib": "^2.4.1",
		"typescript": "^4.9.3",
		"vite": "^4.0.0"
	},
	"type": "module",
	"dependencies": {
		"@apollo/client": "^3.7.12",
		"@prisma/client": "^4.9.0",
		"@sveltejs/adapter-node": "^1.0.0-next.101",
		"apollo-server-svelte-kit": "^3.10.0",
		"bcrypt": "^5.1.0",
		"graphql": "^16.6.0",
		"socket.io": "^4.6.1",
		"socket.io-client": "^4.5.4",
		"svelte-apollo": "^0.5.0",
		"uuid": "^9.0.0",
		"zod-form-data": "^1.2.4"
	}
}
